@article{germond17_raman,
	title = {Raman spectroscopy as a tool for ecology and evolution},
	volume = {14},
	url = {http://rsif.royalsocietypublishing.org/lookup/doi/10.1098/rsif.2017.0174},
	doi = {10.1098/rsif.2017.0174},
	abstract = {{\textless}p{\textgreater}Scientists are always on the lookout for new modalities of information which could reveal new biological features that are useful for deciphering the complexity of biological systems. Here, we introduce Raman spectroscopy as a prime candidate for ecology and evolution. To encourage the integration of this microscopy technique in the field of ecology and evolution, it is crucial to discuss first how Raman spectroscopy fits within the conceptual, technical and pragmatic considerations of ecology and evolution. In this paper, we show that the spectral information holds reliable indicators of intra- and interspecies variations, which can be related to the environment, selective pressures and fitness. Moreover, we show how the technical and pragmatic aspects of this modality (non-destructive, non-labelling, speed, relative low cost, etc.) enable it to be combined with more conventional methodologies. With this paper, we hope to open new avenues of research and extend the scope of available methodologies used in ecology and evolution.{\textless}/p{\textgreater}},
	language = {English},
	number = {131},
	journal = {Journal of the Royal Society Interface},
	author = {Germond, Arno and Kumar, Vipin and Ichimura, Taro and Moreau, Jerome and Furusawa, Chikara and Fujita, Hideaki and Watanabe, Tomonobu M},
	month = jun,
