@article{lei11_multistability,
	title = {Multistability in an age-structured model of hematopoiesis: {Cyclical} neutropenia.},
	volume = {270},
	url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=21094168&retmode=ref&cmd=prlinks},
	doi = {10.1016/j.jtbi.2010.11.024},
	abstract = {Cyclical neutropenia (CN) is a rare hematopoietic disorder in which the patient's neutrophil level drops to extremely low levels for a few days approximately every three weeks. CN is effectively treated with granulocyte colony stimulating factor (G-CSF), which is known to interfere with apoptosis in neutrophil precursors and to consequently increase the circulating neutrophil level. However, G-CSF treatment usually fails to eliminate the oscillation. In this study, we establish an age-structured model of hematopoiesis, which reduces to a set of four delay differential equations with specific forms of initial functions. We numerically investigate the possible stable solutions of the model equations with respect to changes in the parameters as well as the initial conditions. The results show that the hematopoietic system possesses multistability for parameters typical of the normal healthy state. From our numerical results, decreasing the proliferation rate of neutrophil precursors or increasing the stem cell death rate are two possible mechanisms to induce cyclical neutropenia, and the periods of the resulting oscillations are independent of the changing parameters. We also discuss the dependence of the model solution on the initial condition at normal parameter values corresponding to a healthy state. Using insight from our results we design a hybrid treatment method that is able to abolish the oscillations in CN.},
	language = {English},
	number = {1},
	journal = {Journal of Theoretical Biology},
	author = {Lei, Jinzhi and Mackey, Michael C},
	month = feb,
	year = {2011},
	pmid = {21094168},
	pages = {143--153}
}

