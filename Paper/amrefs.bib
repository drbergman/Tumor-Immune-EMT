@article{finkle18_windowed,
	title = {Windowed {Granger} causal inference strategy improves discovery of gene regulatory networks},
	volume = {115},
	url = {http://www.pnas.org/lookup/doi/10.1073/pnas.1710936115},
	doi = {10.1073/pnas.1710936115},
	abstract = {{\textless}p{\textgreater}Accurate inference of regulatory networks from experimental data facilitates the rapid characterization and understanding of biological systems. High-throughput technologies can provide a wealth of time-series data to better interrogate the complex regulatory dynamics inherent to organisms, but many network inference strategies do not effectively use temporal information. We address this limitation by introducing Sliding Window Inference for Network Generation (SWING), a generalized framework that incorporates multivariate Granger causality to infer network structure from time-series data. SWING moves beyond existing Granger methods by generating windowed models that simultaneously evaluate multiple upstream regulators at several potential time delays. We demonstrate that SWING elucidates network structure with greater accuracy in both in silico and experimentally validated in vitro systems. We estimate the apparent time delays present in each system and demonstrate that SWING infers time-delayed, gene–gene interactions that are distinct from baseline methods. By providing a temporal framework to infer the underlying directed network topology, SWING generates testable hypotheses for gene–gene influences.{\textless}/p{\textgreater}},
	language = {English},
	number = {9},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Finkle, Justin D and Wu, Jia J and Bagheri, Neda},
	month = feb,
	year = {2018},
	pmid = {29440433},
	pmcid = {PMC5834671},
	pages = {2252--2257},
	file = {Finkle et al. - 2018 - Windowed Granger causal inference strategy improve.pdf:/Users/adammacl/Zotero/storage/4QGQJ577/Finkle et al. - 2018 - Windowed Granger causal inference strategy improve.pdf:application/pdf}
}

