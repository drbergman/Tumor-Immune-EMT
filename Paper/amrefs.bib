	journal = {PLOS Computational Biology},
	author = {Ye, Yujie and Kang, Xin and Bailey, Jordan and Li, Chunhe and Hong, Tian},
	month = mar,
	year = {2019},
	keywords = {Cell differentiation, Gene expression, Network analysis, Network motifs, Notch signaling, T cells, Topology, Transcriptional control},
	pages = {e1006855},
	file = {Full Text PDF:/Users/adammacl/Zotero/storage/WWTVETPC/Ye et al. - 2019 - An enriched network motif family regulates multist.pdf:application/pdf;Snapshot:/Users/adammacl/Zotero/storage/WXJQ9UHN/article.html:text/html}
}

@article{siegal02_waddington,
	title = {Waddington's canalization revisited: {Developmental} stability and evolution},
	volume = {99},
	copyright = {Copyright © 2002, The National Academy of Sciences},
	issn = {0027-8424, 1091-6490},
	shorttitle = {Waddington's canalization revisited},
	url = {https://www.pnas.org/content/99/16/10528},
	doi = {10.1073/pnas.102303999},
	abstract = {Most species maintain abundant genetic variation and experience a range of environmental conditions, yet phenotypic variation is low. That is, development is robust to changes in genotype and environment. It has been claimed that this robustness, termed canalization, evolves because of long-term natural selection for optimal phenotypes. We show that the developmental process, here modeled as a network of interacting transcriptional regulators, constrains the genetic system to produce canalization, even without selection toward an optimum. The extent of canalization, measured as the insensitivity to mutation of a network's equilibrium state, depends on the complexity of the network, such that more highly connected networks evolve to be more canalized. We argue that canalization may be an inevitable consequence of complex developmental–genetic processes and thus requires no explanation in terms of evolution to suppress phenotypic variation.},
	language = {en},
	number = {16},
	urldate = {2019-04-02},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Siegal, Mark L. and Bergman, Aviv},
	month = aug,
	year = {2002},
	pmid = {12082173},
	pages = {10528--10532},
	file = {Full Text PDF:/Users/adammacl/Zotero/storage/9G938TEQ/Siegal and Bergman - 2002 - Waddington's canalization revisited Developmental.pdf:application/pdf;Snapshot:/Users/adammacl/Zotero/storage/5ERL5L7T/10528.html:text/html}
}

