@article{johnson17_finite,
	title = {The finite state projection approach to analyze dynamics of heterogeneous populations},
	volume = {14},
	issn = {1478-3975},
	url = {https://doi.org/10.1088%2F1478-3975%2Faa6e5a},
	doi = {10.1088/1478-3975/aa6e5a},
	abstract = {Population modeling aims to capture and predict the dynamics of cell populations in constant or fluctuating environments. At the elementary level, population growth proceeds through sequential divisions of individual cells. Due to stochastic effects, populations of cells are inherently heterogeneous in phenotype, and some phenotypic variables have an effect on division or survival rates, as can be seen in partial drug resistance. Therefore, when modeling population dynamics where the control of growth and division is phenotype dependent, the corresponding model must take account of the underlying cellular heterogeneity. The finite state projection (FSP) approach has often been used to analyze the statistics of independent cells. Here, we extend the FSP analysis to explore the coupling of cell dynamics and biomolecule dynamics within a population. This extension allows a general framework with which to model the state occupations of a heterogeneous, isogenic population of dividing and expiring cells. The method is demonstrated with a simple model of cell-cycle progression, which we use to explore possible dynamics of drug resistance phenotypes in dividing cells. We use this method to show how stochastic single-cell behaviors affect population level efficacy of drug treatments, and we illustrate how slight modifications to treatment regimens may have dramatic effects on drug efficacy.},
	language = {en},
	number = {3},
	urldate = {2019-03-26},
	journal = {Physical Biology},
	author = {Johnson, Rob and Munsky, Brian},
	month = may,
	year = {2017},
	pages = {035002},
	file = {IOP Full Text PDF:/Users/adammacl/Zotero/storage/CH2SRNLU/Johnson and Munsky - 2017 - The finite state projection approach to analyze dy.pdf:application/pdf}
}

