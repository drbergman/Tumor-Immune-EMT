	journal = {JCO Clinical Cancer Informatics},
	author = {West, Jeffrey and Robertson-Tessi, Mark and Luddy, Kimberly and Park, Derek S. and Williamson, Drew F.K. and Harmon, Cathal and Khong, Hung T. and Brown, Joel and Anderson, Alexander R.A.},
