@article{dick08_stem,
	title = {Stem cell concepts renew cancer research.},
	volume = {112},
	url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=19064739&retmode=ref&cmd=prlinks},
