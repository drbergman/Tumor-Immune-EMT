	author = {Meyer, Pablo and Cokelaer, Thomas and Chandran, Deepak and Kim, Kyung H and Loh, Po-Ru and Tucker, George and Lipson, Mark and Berger, Bonnie and Kreutz, Clemens and Raue, Andreas and Steiert, Bernhard and Timmer, Jens and Bilal, Erhan and consortium, DREAM 6 7 Parameter Estimation and Stolovitzky, Gustavo and Saez-Rodriguez, Julio},
