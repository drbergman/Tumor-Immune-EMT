@article{hartmann06_modelling,
	title = {Modelling in vitro lung branching morphogenesis during development.},
	volume = {242},
	url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=16808929&retmode=ref&cmd=prlinks},
