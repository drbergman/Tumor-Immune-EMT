@article{babtie10_what,
	title = {What makes an enzyme promiscuous?},
	volume = {14},
	url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=20080434&retmode=ref&cmd=prlinks},
