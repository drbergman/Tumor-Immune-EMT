	pages = {184--197}
}

@article{athanasiadis17_singlecell-1,
	title = {Single-cell {RNA}-{Sequencing} uncovers transcriptional states and fate decisions in haematopoiesis},
	url = {http://biorxiv.org/lookup/doi/10.1101/117960},
	doi = {10.1101/117960},
	abstract = {{\textless}p{\textgreater}The success of marker-based approaches for dissecting haematopoiesis in mouse and human is reliant on the presence of well-defined cell-surface markers specific for diverse progenitor populations. An inherent problem with this approach is that the presence of specific cell surface markers does not directly reflect the transcriptional state of a cell. Here we used a marker-free approach to computationally reconstruct the blood lineage tree in zebrafish and order cells along their differentiation trajectory, based on their global transcriptional differences. Within the population of transcriptionally similar stem and progenitor cells our analysis revealed considerable cell-to-cell differences in their probability to transition to another, committed state. Once fate decision was executed, the suppression of transcription of ribosomal genes and up-regulation of lineage specific factors co-ordinately controlled lineage differentiation. Evolutionary analysis further demonstrated that this haematopoietic program was highly conserved between zebrafish and higher vertebrates.{\textless}/p{\textgreater}},
	language = {English},
	journal = {bioRxiv},
	author = {Athanasiadis, Emmanouil I and Andres, Helena and Botthof, Jan G and Ferreira, Lauren and Lio, Pietro and Cvejic, Ana},
	month = mar,
	year = {2017},
	pages = {117960}
}

