	volume = {139},
	url = {http://dev.biologists.org/cgi/doi/10.1242/dev.083352},
	doi = {10.1242/dev.083352},
	abstract = {{\textless}p{\textgreater}Frizzled (Fz) 2 and Fz7, together with Fz1, form a distinct subfamily within the Frizzled family of Wnt receptors. Using targeted gene deletion, we show that: \textit{Fz7$^{\textrm{{\textbackslash}textminus/{\textbackslash}textminus}}$} mice exhibit tail truncation and kinking with 100\% penetrance and ventricular septal defects (VSDs) with {\textbackslash}textasciitilde15\% penetrance; \textit{Fz2$^{\textrm{+/{\textbackslash}textminus}}$;Fz7$^{\textrm{{\textbackslash}textminus/{\textbackslash}textminus}}$} mice exhibit VSDs with {\textbackslash}textasciitilde50\% penetrance and cleft palate with less than 10\% penetrance; and \textit{Fz2$^{\textrm{{\textbackslash}textminus/{\textbackslash}textminus}}$;Fz7$^{\textrm{{\textbackslash}textminus/{\textbackslash}textminus}}$} mice exhibit convergent extension defects and mid-gestational lethality with 100\% penetrance. When \textit{Fz2} and/or \textit{Fz7} mutations are combined with mutations in \textit{Vangl2, Dvl3, Wnt3a, Wnt5a} or \textit{Wnt11}, an increased frequency of VSDs is observed with \textit{Dvl3, Wnt3a} and \textit{Wnt11}; an increased frequency of palate closure defects is observed with \textit{Vangl2}; and early lethality and enhanced tail shortening are observed with \textit{Wnt5a}. To assess the signaling pathways that underlie these and other Frizzled-mediated genetic interactions, we used transfected mammalian cells to analyze (1) canonical Wnt signaling induced by all pairwise combinations of the ten mouse Frizzleds and the 19 mouse Wnts and (2) localization of each Frizzled at cell-cell junctional complexes formed by mouse Celsr1, a likely indicator of competence for planar cell polarity signaling. These in vitro experiments indicate that Fz2 and Fz7 are competent to signal via the canonical pathway. Taken together, the data suggest that genetic interactions between \textit{Fz2, Fz7} and \textit{Vangl2, Dvl3} and Wnt genes reflect interactions among different signaling pathways in developmental processes that are highly sensitive to perturbations in Frizzled signaling.{\textless}/p{\textgreater}},
	language = {English},
	number = {23},
	journal = {Development},
	author = {Yu, Huimin and Ye, Xin and Guo, Nini and Nathans, Jeremy},
	month = dec,
	year = {2012},
	pmid = {23095888},
	pmcid = {PMC3509732},
	keywords = {Wnt},
	pages = {4383--4394}
}

@article{saito-diaz13_way,
