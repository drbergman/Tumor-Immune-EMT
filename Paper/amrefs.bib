@article{jamieson14_targeting,
	title = {Targeting the $\beta$-catenin nuclear transport pathway in cancer.},
	volume = {27},
	url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=24820952&retmode=ref&cmd=prlinks},
	doi = {10.1016/j.semcancer.2014.04.012},
	abstract = {The nuclear localization of specific proteins is critical for cellular processes such as cell division, and in recent years perturbation of the nuclear transport cycle of key proteins has been linked to cancer. In particular, specific gene mutations can alter nuclear transport of tumor suppressing and oncogenic proteins, leading to cell transformation or cancer progression. This review will focus on one such factor, $\beta$-catenin, a key mediator of the canonical wnt signaling pathway. In response to a wnt stimulus or specific gene mutations, $\beta$-catenin is stabilized and translocates to the nucleus where it binds TCF/LEF-1 transcription factors to transactivate genes that drive tumor formation. Moreover, the nuclear import and accumulation of $\beta$-catenin correlates with clinical tumor grade. Recent evidence suggests that the primary nuclear transport route of $\beta$-catenin is independent of the classical Ran/importin import machinery, and that $\beta$-catenin directly contacts the nuclear pore complex to self-regulate its own entry into the nucleus. Here we propose that the $\beta$-catenin nuclear import pathway may provide an opportunity for identification of specific drug targets and inhibition of $\beta$-catenin nuclear function, much like the current screening of drugs that block binding of $\beta$-catenin to LEF-1/TCFs. Here we will discuss the diverse mechanisms regulating nuclear localization of $\beta$-catenin and their potential as targets for anticancer agent development.},
	language = {English},
	journal = {Seminars in Cancer Biology},
	author = {Jamieson, Cara and Sharma, Manisha and Henderson, Beric R},
	month = aug,
	year = {2014},
	pmid = {24820952},
	pages = {20--29}
}

