@article{buchholz13_disparate,
	title = {Disparate {Individual} {Fates} {Compose} {Robust} {CD}8+ {T} {Cell} {Immunity}},
	volume = {340},
	url = {http://www.sciencemag.org/cgi/doi/10.1126/science.1235454},
	doi = {10.1126/science.1235454},
	abstract = {During an immune response, CD8+ T cells are recruited to provide protection. Most cells differentiate into short-lived effectors that help to clear the pathogen, whereas others form long-lived memory cells to protect against reinfection. Gerlach et al. (p. [635][1], published online 14 March) and Buchholz et al. (p. [630][2], published online 14 March) used in vivo fate mapping of mouse T cells with a defined specificity during a bacterial infection to show that the dynamics of the single-cell response are not uniform. The response of a particular T cell population is the average of a small number of clones that expand greatly (“large clones”) and many clones that only proliferate at low amounts (“small clones”). The memory pool arises largely from small clones whereas effectors are primarily made up of large clones. [1]: /lookup/doi/10.1126/science.1235487 [2]: /lookup/volpage/340/630?iss=6132},
	language = {English},
	number = {6132},
	journal = {Science},
