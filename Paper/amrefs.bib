@article{kiraly15_inflammationinduced,
	title = {Inflammation-{Induced} {Cell} {Proliferation} {Potentiates} {DNA} {Damage}-{Induced} {Mutations} {In} {Vivo}},
	volume = {11},
	url = {http://dx.plos.org/10.1371/journal.pgen.1004901},
	doi = {10.1371/journal.pgen.1004901},
	abstract = {Author Summary People with chronic inflammatory conditions have a markedly increased risk for cancer. In addition, many cancers have an inflammatory microenvironment that promotes tumor growth. Here, we show that inflammatory infiltration synergizes with tissue regeneration to induce DNA sequence rearrangements in vivo. Chronically inflamed issues that are continuously regenerating are thus at an increased risk for mutagenesis and malignant transformation. Further, rapidly dividing tumor cells in an inflammatory microenvironment can also acquire mutations, which have been shown to contribute to drug resistance and disease recurrence. Finally, inflammation-induced tissue regeneration sensitizes tissues to DNA damaging environmental exposures and chemotherapeutics. The work described here thus increases our understanding of how inflammation leads to genetic changes that drive cancer formation and recurrence.},
	language = {English},
	number = {2},
	journal = {PLoS Genetics},
	author = {Kiraly, Orsolya and Gong, Guanyu and Olipitz, Werner and Muthupalani, Sureshkumar and Engelward, Bevin P},
	month = feb,
	year = {2015},
	keywords = {Oncoimmunology, Pancreas},
	pages = {e1004901}
}

