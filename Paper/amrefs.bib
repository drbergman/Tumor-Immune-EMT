	number = {17},
	journal = {Journal of Chemical Physics},
	author = {Prinz, Jan-Hendrik and Wu, Hao and Sarich, Marco and Keller, Bettina and Senne, Martin and Held, Martin and Chodera, John D and Schütte, Christof and Noé, Frank},
