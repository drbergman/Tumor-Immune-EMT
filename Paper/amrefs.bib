	pmid = {28180283},
	pmcid = {PMC5449544},
	pages = {e85},
	file = {PubMed Central Full Text PDF:/Users/adammacl/Zotero/storage/RBDZXNFH/Wang et al. - 2017 - Dynamic chromatin accessibility modeled by Markov .pdf:application/pdf}
}

