	number = {0},
	author = {Man, Jianghong and Yu, Xingjiang and Huang, Haidong and Zhou, Wenchao and Xiang, Chaomei and Huang, Haohao and Miele, Lucio and Liu, Zhenggang and Bebek, Gurkan and Bao, Shideng and Yu, Jennifer S},
	month = nov,
	year = {2017},
