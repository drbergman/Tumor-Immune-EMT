	journal = {eLife},
	author = {Wang, Feng and Shin, JongDae and Shea, Jeremy M and Yu, Jun and Bošković, Ana and Byron, Meg and Zhu, Xiaochun and Shalek, Alex K and Regev, Aviv and Lawrence, Jeanne B and Torres, Eduardo M and Zhu, Lihua J and Rando, Oliver J and Bach, Ingolf and Struhl, Kevin},
