@article{geula15_m6a,
	title = {m6A {mRNA} methylation facilitates resolution of naïve pluripotency toward differentiation.},
	volume = {347},
	url = {http://www.sciencemag.org/content/347/6225/1002.full},
	doi = {10.1126/science.1261417},
