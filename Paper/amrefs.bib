	author = {Wu, Hsing-Jung and Oh, Ji Won and Spandau, Dan F and Tholpady, Sunil and Diaz, Jesus and Schroeder, Laura J and Offutt, Carlos D and Glick, Adam B and Plikus, Maksim V and Koyama, Sachiko and Foley, John},
